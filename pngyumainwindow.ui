  <property name="styleSheet">
   <string notr="true">/* Modern macOS buttons */
QPushButton {
    border: none;
    border-radius: 8px;
    padding: 10px 16px;
    font-size: 13px;
    font-weight: 500;
    background-color: palette(button);
    color: palette(button-text);
}

QPushButton:hover {
    background-color: palette(midlight);
}

QPushButton#pushButton_exec {
    background-color: #007AFF;
    color: white;
    font-weight: 600;
    padding: 12px 24px;
}

QPushButton#pushButton_exec:hover {
    background-color: #0051D5;
}

QPushButton#pushButton_stop_exec {
    background-color: #FF3B30;
    color: white;
    font-weight: 600;
    padding: 12px 24px;
}

QPushButton#pushButton_preview {
    background-color: #007AFF;
    color: white;
    font-weight: 600;
    padding: 8px 16px;
}


QFrame#frame_speed,
QFrame#frame_dithered,
QFrame#frame_colors,
QFrame#frame_output_filename,
QFrame#frame_ie6
{
    border: 1px solid rgba(127,127,127,63);
    border-radius: 5px;
    padding: 2px;
}

/* Modern segmented controls */
QToolButton {
    border: 1px solid palette(mid);
    padding: 8px 16px;
    background-color: palette(button);
    color: palette(button-text);
    font-size: 13px;
    font-weight: 500;
}

QToolButton:checked {
    background-color: #007AFF;
    color: white;
    border-color: #007AFF;
}

QToolButton:hover:!checked {
    background-color: palette(midlight);
}

QToolButton#toolButton_compress_option_default,
QToolButton#toolButton_output_option_overwrite_original {
    border-top-left-radius: 8px;
    border-bottom-left-radius: 8px;
    border-right: none;
}

/*middle*/
QToolButton#toolButton_background_black
{
  border-style: outset;
  border : 1px solid rgb(127,127,127,127);
  border-radius : 0px;
  padding: 3px;
  background : qlineargradient(spread:reflect, x1:0, y1:1, x2:0, y2:0,
    stop:0 rgba(127, 127, 127, 64), stop:1 rgba(255, 255, 255, 100));
}

QToolButton#toolButton_compress_option_custom,
QToolButton#toolButton_output_option_custom {
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    border-left: none;
}

/* Modern table styling */
QTableWidget {
    background-color: palette(base);
    border: none;
    border-radius: 10px;
    selection-background-color: palette(highlight);
    selection-color: palette(highlighted-text);
    font-size: 13px;
    alternate-background-color: palette(alternate-base);
    gridline-color: palette(mid);
}

QTableWidget::item {
    padding: 10px 8px;
    border: none;
}

QHeaderView::section {
    background-color: palette(button);
    color: palette(button-text);
    padding: 10px 8px;
    border: none;
    border-right: 1px solid rgba(128, 128, 128, 0.3);
    font-weight: 600;
    font-size: 12px;
}

QHeaderView::section:last {
    border-right: none;
}

/* Modern text fields */
QLineEdit {
    border: 1px solid rgba(0, 0, 0, 0.1);
    border-radius: 8px;
    padding: 8px 12px;
    background-color: palette(base);
    color: palette(text);
    font-size: 13px;
}

QLineEdit:focus {
    border-color: #007AFF;
    border-width: 2px;
}

QLineEdit[drp_enabled="true"] {
    background-color: #007AFF;
    color: white;
    border-color: #007AFF;
}

/* Modern labels */
QLabel {
    color: palette(text);
    font-size: 13px;
}

QLabel#label_drop_here {
    background-color: palette(base);
    color: palette(mid);
    font-size: 15px;
    font-weight: 500;
}

/* Status colors */
QLabel#label_current_saved_size {
    color: palette(mid);
    font-size: 12px;
}

QLabel#label_current_saved_size[color_positive="true"] {
    color: #30D158;
}

QLabel#label_current_saved_size[color_negative="true"] {
    color: #FF453A;
}

/* Modern frames */
QFrame#frame_speed,
QFrame#frame_dithered,
QFrame#frame_colors,
QFrame#frame_output_filename,
QFrame#frame_ie6 {
    border: 1px solid rgba(0, 0, 0, 0.08);
    border-radius: 10px;
    padding: 8px;
    background-color: rgba(0, 0, 0, 0.02);
}

/* Add file button - specific styling to override general QToolButton */
QToolButton#toolButton_add_file {
    border: 1px solid rgba(0, 0, 0, 0.1) !important;
    border-radius: 6px !important;
    background-color: palette(button) !important;
    color: #007AFF !important;
    font-weight: 600 !important;
    font-size: 16px !important;
    padding: 2px !important;
}

QToolButton#toolButton_add_file:hover {
    background-color: palette(midlight) !important;
    color: #0051D5 !important;
}

QToolButton#toolButton_add_file:pressed {
    background-color: palette(mid) !important;
    color: #003D82 !important;
}

QToolButton#toolButton_add_file:checked {
    background-color: palette(button) !important;
    color: #007AFF !important;
}

/* Clear button - adaptive light/dark mode styling */
QPushButton#pushButton_filelist_clear {
    border: 1px solid palette(mid);
    border-radius: 6px;
    background-color: palette(button);
    color: palette(button-text);
    padding: 4px 8px;
}

QPushButton#pushButton_filelist_clear:hover {
    background-color: palette(midlight);
    border-color: palette(dark);
}

QPushButton#pushButton_filelist_clear:pressed {
    background-color: palette(mid);
    color: palette(bright-text);
}</string>
